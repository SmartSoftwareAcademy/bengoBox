{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Spring": {
    "Application": {
      "Name": "TDBSOFT.APIGATEWAY"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": {
        "DefaultZone": "http://localhost:8761/eureka/"
      },
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true
    },
    "Instance": {
      "NonSecurePort": 9500,
      "HostName": "localhost",
      "InstanceId": "TDBSoftAPIGateway,Port:9500"
    }
  },
  "AllowedHosts": "*"
}
